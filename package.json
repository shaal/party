{
  "name": "devparty",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "build": "yarn generate && next build",
    "start": "next start -H 0.0.0.0 -p ${PORT-8080}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "generate": "prisma generate && yarn codegen",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:seedtopics": "ts-node prisma/seedtopics.ts",
    "codegen": "yarn codegen:schema && yarn codegen:graphql",
    "codegen:schema": "ts-node --transpile-only -r tsconfig-paths/register src/graphql/index.ts",
    "codegen:graphql": "graphql-codegen",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@giraphql/core": "^2.16.0",
    "@giraphql/plugin-directives": "^2.8.0",
    "@giraphql/plugin-prisma": "^0.10.0",
    "@giraphql/plugin-relay": "^2.16.0",
    "@giraphql/plugin-scope-auth": "^2.11.0",
    "@giraphql/plugin-simple-objects": "^2.9.0",
    "@giraphql/plugin-validation": "^2.12.0",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "@hookform/resolvers": "^2.8.2",
    "@prisma/client": "3.3.0",
    "@rehooks/online-status": "^1.1.2",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@walletconnect/web3-provider": "^1.6.6",
    "clsx": "^1.1.1",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.1",
    "graphql": "^15.7.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-helix": "1.8.4",
    "graphql-rate-limit-directive": "^1.3.0",
    "hash-wasm": "^4.9.0",
    "ioredis": "^4.28.0",
    "linkify-plugin-hashtag": "^3.0.3",
    "linkify-plugin-mention": "^3.0.3",
    "linkify-react": "^3.0.3",
    "linkifyjs": "^3.0.3",
    "markdown-to-jsx": "^7.1.3",
    "next": "^12.0.1",
    "next-iron-session": "^4.2.0",
    "next-pwa": "^5.4.0",
    "next-themes": "^0.0.15",
    "nextjs-progressbar": "^0.0.11",
    "octokit": "^1.7.0",
    "react": "^17.0.2",
    "react-cool-inview": "^2.0.8",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.18.0",
    "react-hot-toast": "^2.1.1",
    "request-ip": "^2.1.3",
    "spotify-web-api-node": "^5.0.2",
    "swr": "^1.0.1",
    "timeago.js": "^4.0.2",
    "unfurl.js": "^5.6.2",
    "web3modal": "^1.9.4",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.1.6",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.4.0-alpha.1",
    "@tailwindcss/typography": "^0.5.0-alpha.2",
    "@types/faker": "^5.5.9",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/ioredis": "^4.27.8",
    "@types/linkifyjs": "^2.1.4",
    "@types/react": "^17.0.33",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.10",
    "@types/request-ip": "^0.0.37",
    "@types/spotify-web-api-node": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "autoprefixer": "^10.3.7",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "faker": "^5.5.3",
    "hplipsum": "^1.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prisma": "3.3.0",
    "tailwindcss": "^3.0.0-alpha.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
